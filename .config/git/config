[user]
    name = zubieta
    email = zubieta@github.com
[alias]
    br = branch
    bra = branch -a
    ci = commit
    cm = commit -m
    co = checkout
    diffstat = diff --stat
    last = log -1 HEAD
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    ls = ls-files
    st = status
    stashdiff = stash show -p
    track = branch -u
    unstage = reset HEAD --
    unstash = stash pop
    upstream = "!git status -sb | grep ^## | cut -s -d. -f4"
    fire = "!export TIME=$(date +%Y%m%dT%H%M%S); export HOST=$(hostname); git checkout -b fire-branch-$USER-$HOST-$TIME && git add --all && git commit -m \"Have to run\" && git push -u origin fire-branch-$USER-$HOST-$TIME"
[filter "lfs"]
    clean = git-lfs clean -- %f
    process = git-lfs filter-process
    required = true
    smudge = git-lfs smudge -- %f
[core]
    editor = vim
[protocol "keybase"]
    allow = always
[status]
    submodulesummary = false
[credential "https://source.developers.google.com"]
    helper = gcloud.sh
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[include]
    path = ~/.config/git/local
