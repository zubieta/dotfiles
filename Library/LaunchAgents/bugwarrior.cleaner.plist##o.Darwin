<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>bugwarrior.cleaner</string>
        <key>ProgramArguments</key>
        <array>
            <string>rm</string>
            <string>-f</string>
            <string>/Users/czr/.task/bugwarrior.lockfile</string>
        </array>
        <key>RunAtLoad</key>
        <true />
        <key>LaunchOnlyOnce</key>
        <true />
        <key>StandardOutPath</key>
        <string>/Users/czr/.local/var/log/bugwarrior.log</string>
    </dict>
</plist>
